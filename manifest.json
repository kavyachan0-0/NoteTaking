{
    "name": "Note App",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": "./logo.png"
    },
    "permissions": ["storage", "tabs", "windows"],
    "icons": {
      "128": "./logo.png"
    }
  }
  
  
  